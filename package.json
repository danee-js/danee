{
  "name": "danee",
  "description": "Dynamic, Automated, Natural, Elegant and Efficient",
  "version": "0.0.0",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
